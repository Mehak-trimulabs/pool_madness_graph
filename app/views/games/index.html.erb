<%= render :partial => 'brackets/bracket' %>

<% if can? :update, Game %>
<script>
    $('.match').click(function(){
        var game_id = $(this)[0].id.replace('match', '');
        window.location = "<%= games_url %>/" + game_id;
    });


    $(document).ready(function() {
       $('.match').addClass('edit-game')
    });
</script>
<% end %>